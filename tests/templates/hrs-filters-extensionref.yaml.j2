
---

apiVersion: gateway.networking.k8s.io/v1beta1
kind: HTTPRoute
metadata:
  name: test-filter-extensionref
spec:
  parentRefs:
  - name: gateway
    sectionName: http
  hostnames:
  - {{ hostname }}
  rules:
  - filters:
      - type: ExtensionRef
        extensionRef:
          group: ""
          kind: Service
          name: test-service